doctype html
html(lang="en")
    head

        <!-- Load tailwind for production -->
        link(rel='stylesheet', href='/assets/css/output.css?v=' + version)

        <!-- When fonts are finalized load them properly -->
        link(rel="preconnect" href="https://fonts.googleapis.com")
        link(rel="preconnect" href="https://fonts.gstatic.com" crossorigin)
        link(href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700&display=swap" rel="stylesheet")

        meta(name="viewport" content="width=device-width, initial-scale=1")

        title().
            !{code} Not Found

    body(class='antialiased bg-slate-200 '+bodyClass)

        include nav

        <!-- React Components -->
        section(class="leading-6 text-gray-700 sm:px-6 lg:px-8")
            div(class="w-full mx-auto duration-500 ease-in-out transform max-w-screen-xl px-14 py-14")
                h1(class="font-medium text-4xl max-w-screen-xl").
                    Not found !{url}

        <!-- Webpack -->
        script(src='/dist/index.js?v='+version)
        include foot
